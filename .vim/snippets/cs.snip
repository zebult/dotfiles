snippet generic
    public List <T> Build <T>() where T : AbstractFactoryModel, new()
    {
        var factoryModelList  = new List <T>();
        var type              = typeof(T) == typeof(SameFactoryModel) ? ObjectType.Same : ObjectType.Food;
        var factoryEntityList = SVC.Entity.GetObjectFactoryList(this.Key, type);

        for (int i = 0; i < factoryEntityList.Count; i++)
        {
            var model = new T();
            model.Init(factoryEntityList[i], FactoryObject, PlayerSame, InitCount);

            factoryModelList.Add(model);
        }


        return factoryModelList;
    }

